/*
    JaCaMo project file
*/

mas jaca {

    // Agents initialization
    agent auctioneer: auctioneer.asl{
    	beliefs: startPrice(0), increment(1)
        join: room
        focus: room.product
    }

    agent bidder: bidder.asl{
        join: room
        focus: room.product
		instances: 50
    } 

    // Environment workspaces
     workspace room {
    	artifact product : auction.artifact()
    }

}